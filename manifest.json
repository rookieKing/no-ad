{
  "name": "no-ad",
  "content_scripts": [
    {
      "matches": ["*://*.csdn.net/*"],
      "js": ["csdn.net/index.js"]
    },
    {
      "matches": ["*://blog.csdn.net/*"],
      "js": ["csdn.net/blog.js"],
      "css": ["csdn.net/blog.css"]
    }
  ],
  "version": "1.0",
  "permissions": ["activeTab"],
  "manifest_version": 2
}
