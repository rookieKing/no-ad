{
  "name": "no-ad",
  "content_scripts": [
    {
      "matches": ["*://uiadmin.net/*"],
      "css": ["uiadmin.net/main.css"]
    },
    {
      "matches": ["*://codesandbox.io/*"],
      "css": ["codesandbox.io/main.css"]
    },
    {
      "matches": ["*://*.gamepedia.com/*"],
      "css": ["gamepedia.com/pathofexile.css"]
    },
    {
      "matches": ["*://poedb.tw/*"],
      "css": ["poedb.tw/main.css"]
    },
    {
      "matches": ["*://poe.ninja/*"],
      "css": ["poe.ninja/main.css"]
    },
    {
      "matches": ["*://juejin.im/*"],
      "css": ["juejin.im/entry.css"]
    },
    {
      "matches": ["*://www.jb51.net/softs/*"],
      "css": ["jb51.net/www.softs.css"]
    },
    {
      "matches": ["*://blog.popkx.com/*"],
      "js": ["popkx.com/blog.js"]
    },
    {
      "matches": ["*://www.runoob.com/*"],
      "css": ["runoob.com/www.css"]
    },
    {
      "matches": ["*://blog.51cto.com/*"],
      "css": ["51cto.com/blog.css"]
    },
    {
      "matches": ["*://wenku.baidu.com/*"],
      "js": ["baidu.com/wenku.js"],
      "css": ["baidu.com/wenku.css"]
    },
    {
      "matches": ["*://*.csdn.net/*"],
      "js": ["csdn.net/index.js"]
    },
    {
      "matches": ["*://blog.csdn.net/*"],
      "js": ["csdn.net/blog.js"],
      "css": ["csdn.net/blog.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["*://blog.csdn.net/*"],
      "resources": ["csdn.net/inject.js"]
    }
  ],
  "version": "1.0",
  "description": "no-ad!",
  "manifest_version": 3
}
